<% user_items.each do |item| %>
  <%= item.name %>
  <%=  link_to "completed", user_item_path(user_id: @user.id, id: item.id), method: :delete, class: 'glyphicon glyphicon-ok' %>
<% end %>
<div>
    <h4 class='list-group-item-heading'><%= @item.name %></h4>
    <p class='list-group-item-text'><%= time_left(@item) %></p>
</div>
